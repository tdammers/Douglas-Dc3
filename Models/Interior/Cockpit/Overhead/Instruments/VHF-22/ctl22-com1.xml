      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 30 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.0000 </x-m>
      <y-m> -0.0138 </y-m>
      <z-m> -0.0183 </z-m>
    </center>
    <axis>
